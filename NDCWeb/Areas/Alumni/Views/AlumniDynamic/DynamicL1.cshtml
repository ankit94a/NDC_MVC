
@{
    ViewBag.Title = "DynamicL1";
}

<section class="pt-1">
    <div class="container">
        <ol class="breadcrumb shadow custom-header">
            <li class="breadcrumb-item"><a href='@Url.Action("Index","Home")' class="text-white">Home</a></li>
            <li class="breadcrumb-item"><a href="#" class="text-white"> @ViewBag.Heading</a></li>
        </ol>
    </div>
</section>
<section class="pb20">
    <div class="container">
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="content-area">
                    <div class="clearfix"></div>
                    <div class="field-content">
                        @Html.Raw(ViewBag.Body)
                    </div>
                </div>

            </div>
        </div>
    </div>
</section>


