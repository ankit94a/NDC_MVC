@model NDCWeb.Areas.Member.View_Models.CrsMemberPersonalIndxVM
<table class="table table-condensed table-bordered">
    <tr>
        <td><img src="@Url.Content(Model.MemberImgPath)" class="img-thumbnail" width="120" /></td>
        <td colspan="4" class="text-center">
            <img src="/images/NDC-Logo.png" width="80" /><br />
            <h4>
                <u>PERSONAL DETAILS</u>
            </h4>
        </td>
        <td colspan="1" style="text-align:right"><img src="@Url.Content(Model.JointImgPath)" class="img-thumbnail" width="120" /></td>
    </tr>
    <tr>
        <td class="juipfname"> @Html.DisplayNameFor(model => model.FirstName)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.FirstName)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.MiddleName) </td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.MiddleName)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.Surname)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.Surname)</td>
    </tr>
    <tr>
        <td class="juipfname"> @Html.DisplayNameFor(model => model.FatherName)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.FatherName)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.FatherMiddleName) </td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.FatherMiddleName)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.FatherSurname)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.FatherSurname)</td>
    </tr>
    <tr>
        <td class="juipfname"> @Html.DisplayNameFor(model => model.MotherName)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.MotherName)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.MotherMiddleName) </td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.MotherMiddleName)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.MotherSurname)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.MotherSurname)</td>
    </tr>
    <tr>
        <td class="juipfname">@Html.DisplayNameFor(model => model.NickName)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.NickName)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.MobileNo)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.MobileNo)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.AlternateMobileNo)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.AlternateMobileNo)</td>
    </tr>
    <tr>
        <td class="juipfname">@Html.DisplayNameFor(model => model.IndentificationMark)</td>
        <td class="juipfvalue"> @Html.DisplayFor(model => model.IndentificationMark)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.EmailId)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.EmailId) </td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.AlternateEmailId)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.AlternateEmailId)</td>
    </tr>
    <tr>
        <td class="juipfname">@Html.DisplayNameFor(model => model.DOBirth)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.DOBirth)</td>
        <td class="juipfname">Marital Status</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.MaritalStatus)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.DOMarriage)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.DOMarriage)</td>
    </tr>
    <tr>
        <td class="juipfname">Gender</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.Gender)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.BloodGroup)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.BloodGroup)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.Height)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.Height)</td>
    </tr>
    <tr>
        <td class="juipfname">CitizenshipCountry</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.CitizenshipCountries.CountryName)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.VoterIdNo)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.VoterIdNo)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.PANCardNo)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.PANCardNo)</td>
    </tr>
    <tr class="bg-2 text-white">
        <td colspan="6"><b>Address for Communication</b> (@Html.DisplayFor(model => model.CommunicationAddress))</td>


    </tr>
    <tr>
        <td class="juipfname">Flat/House/Door No.</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.OfficeHouseNo)</td>
        <td class="juipfname">Name of Premises/Building</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.OfficePremisesName)</td>
        <td class="juipfname">Area/Locality/Post Office</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.OfficeArea)</td>
    </tr>
    <tr>
        <td class="juipfname">Road/Street/Lane</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.OfficeStreet)</td>
        <td class="juipfname">Town/city/District</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.OfficeCity)</td>
        <td class="juipfname">State/Union Territory</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.OfficeStates.StateName)</td>
    </tr>
    <tr>
        <td class="juipfname">Country</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.OfficeStates.Countries.CountryName)</td>
        <td class="juipfname">Pin/Zip</td>
        <td class="juipfvalue" colspan="3">@Html.DisplayFor(model => model.OfficeZipCode)</td>
    </tr>

    <tr class="bg-2 text-white">
        <td colspan="6"><b>Passport Detail</b></td>
    </tr>
    <tr>
        <td class="juipfname">@Html.DisplayNameFor(model => model.HoldingPassport)</td>
        <td class="juipfvalue">
            @{ if (Model.HoldingPassport == null || Model.HoldingPassport == "No")
                { <text>Not Holding HoldingPassport or Official</text> }
            else if (Model.HoldingPassport == "Yes")
            {
                <text>Deplomatic or Official</text>
            }
            }
        </td>
        <td class="juipfname"> @Html.DisplayNameFor(model => model.MemberPassportNo)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.MemberPassportNo)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.MemberPassportIssueDate) </td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.MemberPassportIssueDate)</td>
    </tr>
    <tr>
        <td class="juipfname">@Html.DisplayNameFor(model => model.MemberPassportValidUpto) </td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.MemberPassportValidUpto)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.MemberPassportType) </td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.MemberPassportType)</td>
        <td class="juipfname">PASSPORT FILE (Front) </td>
        <td class="juipfvalue" colspan="1">
            <a href="@Url.Content(Model.MemberPassportImgPath ?? "#")">Front Side</a>
            |
            <a href="@Url.Content(Model.MemberPassportBackImgPath ?? "#")">Back Side</a>
        </td>
    </tr>

    @if (Model.HoldingPersonalPassportSelf == true)
    {
        <tr class="bg-2 text-white">
            <td colspan="6"><b>Personal Passport Detail</b></td>
        </tr>
        <tr>
            <td class="juipfname"> @Html.DisplayNameFor(model => model.MemberPersonalPassportNo)</td>
            <td class="juipfvalue">@Html.DisplayFor(model => model.MemberPersonalPassportNo)</td>
            <td class="juipfname">@Html.DisplayNameFor(model => model.MemberPersonalPassportIssueDate) </td>
            <td class="juipfvalue">@Html.DisplayFor(model => model.MemberPersonalPassportIssueDate)</td>
            <td class="juipfname">@Html.DisplayNameFor(model => model.MemberPersonalPassportValidUpto) </td>
            <td class="juipfvalue">@Html.DisplayFor(model => model.MemberPersonalPassportValidUpto)</td>
        </tr>
        <tr>
            <td class="juipfname">PASSPORT FILE (Front) </td>
            <td class="juipfvalue" colspan="1">
                <a href="@Url.Content(Model.MemberPersonalPassportImgPath ?? "#")">Front Side</a>
                |
                <a href="@Url.Content(Model.MemberPersonalPassportBackImgPath ?? "#")">Back Side</a>
            </td>
        </tr>
    }

    @if (Model.CitizenshipCountries.CountryName != "India")
    {
        <tr class="bg-2 text-white">
            <td colspan="6"><b>Visa Detail</b></td>
        </tr>
        <tr>
            <td class="juipfname"> @Html.DisplayNameFor(model => model.VisaNo)</td>
            <td class="juipfvalue">@Html.DisplayFor(model => model.VisaNo)</td>
            <td class="juipfname">@Html.DisplayNameFor(model => model.VisaIssueDate) </td>
            <td class="juipfvalue">@Html.DisplayFor(model => model.VisaIssueDate)</td>
            <td class="juipfname">@Html.DisplayNameFor(model => model.VisaValidUpto) </td>
            <td class="juipfvalue">@Html.DisplayFor(model => model.VisaValidUpto)</td>
        </tr>
        <tr>
            <td class="juipfname">VISA FILE </td>
            <td class="juipfvalue" colspan="7"><a href="@Url.Content(Model.VisaPath ?? "#")">Visa</a></td>
        </tr>
    }
    <tr class="bg-2 text-white">
        <td colspan="6"><b>FRRO Detail</b></td>
    </tr>
    <tr>
        <td class="juipfname">@Html.DisplayNameFor(model => model.SelfFRRONo) </td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.SelfFRRONo)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.SelfIssueDate) </td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.SelfIssueDate)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.SelfValidUpto) </td>
        <td class="juipfvalue" colspan="4">@Html.DisplayFor(model => model.SelfValidUpto)</td>
    </tr>
    <tr>
        <td class="juipfname">FRRO images </td>
        <td class="juipfvalue" colspan="7"><a href="@Url.Content(Model.SelfFRROPath ?? "#")">FRRO</a></td>
    </tr>

    <tr class="bg-2 text-white">
        <td colspan="6"><b>Biographical Sketch</b></td>
    </tr>
    <tr>
        <td class="juipfvalue" colspan="6">
            @Html.DisplayFor(model => model.BioSketch)
            <a class="btn btn-transparent btn-sm float-right" data-toggle="tab" href="#PersonalDetailPartial" onclick="editTab('PersonalDetailPartial')"><i class="fa fa-pencil-square text-primary"></i></a>
        </td>
    </tr>
</table>

