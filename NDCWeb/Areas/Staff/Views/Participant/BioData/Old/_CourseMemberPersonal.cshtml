@model NDCWeb.Areas.Member.View_Models.CrsMemberPersonalIndxVM
<div class="card mt-4">
    <div class="card-body">
        <table class="table table-condensed table-bordered table-hover">
            <tr>
                <td><img src="@Url.Content(Model.MemberImgPath)" class="img-thumbnail" width="120" /></td>
                <td colspan="6" class="text-center">
                    <img src="/images/NDC-Logo.png" width="80" /><br />

                </td>
                <td colspan="2"><img src="@Url.Content(Model.JointImgPath)" class="img-thumbnail" width="120" /></td>
            </tr>
            <tr class="bg-light">
                <td colspan="7"><u>PERSONAL DETAILS</u></td>
                <td colspan="2"><a class="btn btn-danger btn-sm" data-toggle="tab" href="#PersonalDetailPartial" onclick="editTab('PersonalDetailPartial')">EDIT THIS SECTION</a></td>
            </tr>
            <tr>
                <td class="juipfname"> @Html.DisplayNameFor(model => model.FirstName)</td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.FirstName)</td>
                <td class="juipfname">@Html.DisplayNameFor(model => model.MiddleName) </td>
