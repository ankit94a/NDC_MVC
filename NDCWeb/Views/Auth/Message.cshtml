
@{
    ViewBag.Title = "Message";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAuth.cshtml";
}

<div class="bg-contact2" style="background-image: url('/Component/css/images/bg_01.jpg');">
    <div class="container-contact2">
        <div class="wrap-contact2">
            <div class="card text-center shadow">
                <div class="card-header text-uppercase text-success">
                    <h3><i class="fa fa-check-circle"></i>Request Submitted Successfully </h3>
                </div>
                <div class="card-body">
                    <h5>Your reset password request has been sent to NDC Website Administrator.</br> Please contact NDC Website Administrator/GSO Systems to get your password reset.</h5><br />
                    <a href="@Url.Action("Login","Auth")" class="btn-lg btn btn-link">Back to Login Page</a>

                </div>
            </div>
        </div>
    </div>
</div>

