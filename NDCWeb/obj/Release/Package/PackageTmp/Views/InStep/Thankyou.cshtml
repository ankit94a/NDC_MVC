
@{
    ViewBag.Title = "Thankyou";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="bg-contact2" style="background-image: url('/Component/css/images/bg_01.jpg');">
    <div class="container-contact2">
        <div class="wrap-contact2">
            @if (null != TempData["alertMessage"])
            {
                <div class="card text-center shadow">
                    <div class="card-header text-uppercase text-success">
                        <h3><i class="fa fa-check-circle"></i>Application Submitted Successfully </h3>
                    </div>
                    <div class="card-body">
                        <h2 class="badge-success bg-success">Refrence No : @TempData["refNo"].ToString()</h2><br />
                        @TempData["alertMessage"].ToString()
                        <div>
                            <a href="@Url.Action("Index","Home")">Back to home</a>
                        </div>
                    </div>

                </div>
            }
        </div>
    </div>
</div>