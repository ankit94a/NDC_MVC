@*@model NDCWeb.Areas.Member.View_Models.CrsMbrNameTallyVM*@
@model NDCWeb.Areas.Member.View_Models.TallyDetailIndxVM

<div class="card mt-4">
    <div class="card-header">
        ADMINISTRATION
    </div>
    <div class="card-body">
        <a class="btn btn-danger btn-sm float-right" data-toggle="tab" href="#TallyPartial" onclick="editTab('TallyPartial')">EDIT THIS SECTION</a>
        <table class="table table-condensed table-bordered table-hover">
            <tr class="bg-light">
                <td colspan="10">Name Tally</td>

            </tr>
            <tr>
                <td class="juipfname">@Html.DisplayNameFor(model => model.RankAbbr)</td>
                <td class="juipfvalue"> @Html.DisplayFor(model => model.RankAbbr)</td>
                <td class="juipfname"> @Html.DisplayNameFor(model => model.PassportName)</td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.PassportName)</td>
                <td class="juipfname">@Html.DisplayNameFor(model => model.TabName) </td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.TabName)</td>
                <td class="juipfname">@Html.DisplayNameFor(model => model.NickName) </td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.NickName)</td>
                <td class="juipfname">@Html.DisplayNameFor(model => model.CountryService) </td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.CountryService)</td>
            </tr>
            <tr class="bg-light">
                <td colspan="10">Tele Directory of NDC Course (Current)</td>
            </tr>
            <tr>
                <td class="juipfname">@Html.DisplayNameFor(model => model.NameORRank) </td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.NameORRank)</td>
                <td class="juipfname">@Html.DisplayNameFor(model => model.ResidentialAddress) </td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.ResidentialAddress)</td>
                <td class="juipfname">@Html.DisplayNameFor(model => model.MobileNo) </td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.MobileNo)</td>
                <td class="juipfname">@Html.DisplayNameFor(model => model.TelephoneNo) </td>
                <td class="juipfvalue" colspan="3">@Html.DisplayFor(model => model.TelephoneNo)</td>
            </tr>
            <tr class="bg-light">
                <td colspan="10">Vehicle Sticker (For Indian Officers Only)</td>
            </tr>
            <tr>
                <td class="juipfname">@Html.DisplayNameFor(model => model.BrandModelNo) </td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.BrandModelNo)</td>
                <td class="juipfname">@Html.DisplayNameFor(model => model.Colour) </td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.Colour)</td>
                <td class="juipfname">@Html.DisplayNameFor(model => model.RegistrationNo) </td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.RegistrationNo)</td>
                <td class="juipfname">@Html.DisplayNameFor(model => model.DrivingLicenseNo) </td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.DrivingLicenseNo)</td>
                <td class="juipfname">@Html.DisplayNameFor(model => model.NoOfVehicle) </td>
                <td class="juipfvalue">@Html.DisplayFor(model => model.NoOfVehicle)</td>
            </tr>
            <tr>
                <td colspan="5">Image(s) Registration Certificate:</td>
                <td colspan="5">Image(s) Driving License:</td>
            </tr>
            <tr>
                <td class="juipfname" colspan="5"><img src="@Url.Content(Model.RegistrationCertificatePath)" class="img-thumbnail" width="120" /></td>
                <td class="juipfvalue" colspan="5"><img src="@Url.Content(Model.RegistrationCertificatePath)" class="img-thumbnail" width="120" /></td>
            </tr>
        </table>
    </div>

</div>



