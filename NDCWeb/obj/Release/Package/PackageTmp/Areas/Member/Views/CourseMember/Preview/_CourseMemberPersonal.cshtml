@model NDCWeb.Areas.Member.View_Models.CrsMemberPersonalIndxVM
<table class="table table-condensed table-bordered">
    <tr>
        <td>
            @if (Model.MemberImgPath != null && Model.MemberImgPath != "#")
            {
            <img src="@Url.Content(Model.MemberImgPath)" class="img-thumbnail" width="120" />
            }
            else
            {
                @: <b>N/A</b>
            }
            
        </td>
        <td colspan="4" class="text-center">
            <img src="/images/NDC-Logo.png" width="80" /><br />
            <h4>
                <u>PERSONAL DETAILS</u>
            </h4>
        </td>
        <td style="text-align:right">
            @if (Model.JointImgPath != null && Model.JointImgPath != "#")
            {
            <img src="@Url.Content(Model.JointImgPath)" class="img-thumbnail" width="120" />
            }
            else
            {
                @: <b>N/A</b>
            }
            
        </td>
    </tr>
    </table>
