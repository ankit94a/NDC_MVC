    {
        <tr class="bg-2 text-white">
            <td colspan="6"><b>Personal Passport Detail</b></td>
        </tr>
        <tr>
            <td class="juipfname"> @Html.DisplayNameFor(model => model.MemberPersonalPassportNo)</td>
            <td class="juipfvalue">@Html.DisplayFor(model => model.MemberPersonalPassportNo)</td>
