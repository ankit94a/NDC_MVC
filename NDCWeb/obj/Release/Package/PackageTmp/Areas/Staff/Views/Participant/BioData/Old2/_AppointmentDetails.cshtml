        <td colspan="6">Present Appointment Details</td>
    </tr>
    <tr>
        <td class="juipfname">@Html.DisplayNameFor(model => model.Designation)</td>
        <td class="juipfvalue"> @Html.DisplayFor(model => model.Designation)</td>
        <td class="juipfname"> @Html.DisplayNameFor(model => model.Organisation)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.Organisation)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.Location) </td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.Location)
            <a class="btn btn-transparent btn-sm float-right" data-toggle="tab" href="#ServicePartial" onclick="serviceEditClick('AppointmentPartial', 'E')"><i class="fa fa-pencil-square text-primary"></i></a></td>
    </tr>
</table>
