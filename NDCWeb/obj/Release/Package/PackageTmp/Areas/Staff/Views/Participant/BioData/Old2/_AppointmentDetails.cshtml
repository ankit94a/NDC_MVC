@model NDCWeb.Areas.Member.View_Models.CrsMbrAppointmentVM
<table class="table table-condensed table-bordered">
    <tr class="bg-2 text-white">
        <td colspan="6">Service Details</td>
    </tr>
    <tr class="bg-light text-center">
        <td class="juipfname">Branch of Service</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.Service)</td>
        <td class="juipfname">@Html.DisplayNameFor(model => model.ServiceNo)</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.ServiceNo)</td>
        <td class="juipfname">Rank/Status</td>
        <td class="juipfvalue">@Html.DisplayFor(model => model.Ranks.RankName)</td>
    </tr>
    <tr>
        <td class="juipfname">@Html.DisplayNameFor(model => model.Branch)</td>
